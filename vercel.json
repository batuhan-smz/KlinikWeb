{
  "version": 2,
  "builds": [
    {
      "src": "KlinikWebsite/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "KlinikWebsite/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "KlinikWebsite.settings"
    }
  }
}
